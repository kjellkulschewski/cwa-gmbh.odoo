<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="test_action" model="ir.actions.act_window">
        <field name="name">Test action</field>
        <field name="res_model">estate.property</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="Estate_tree" model="ir.ui.view">
        <field name="name">estate.tree</field>
        <field name="model">estate.property</field>
        <field name="arch" type="xml">
            <tree string="Channel">
                <field name="name" string="Title" width="100px"/>
                <field name="postcode" string="Postcode" width="100px"/>
                <field name="bedrooms" string="Bedrooms" width="100px"/>
                <field name="living_area" string="Living Area (sqm)" width="100px"/>
                <field name="expected_price" string="Expected Price" width="100px"/>
                <field name="selling_price" string="Selling Price" width="100px"/>
                <field name="date_availability" string="Available From" width="100px"/>
            </tree>
        </field>
    </record>
</odoo>